void verif_identity_cln(int *grille_test, int taille, bool *verif) {
    taille = sqrt(taille);
    for (int array[taille][taille]) {
        while (*verif) {
            for (int ncln = 0; ncln < taille; ncln++)
                for (int test_cln = ncln + 1; test_cln < ncln; test_cln++) {
                    bool identity = true;
                    for (int row = 0; row < taille && identity; row++)
                        if (array[ncln][row] != array[test_cln][row])
                            identity = false;
                    if (identity)
                        printf("identity cln %d and %d\n", ncln, test_cln);
                    else
                        printf("no identity cn %d and %d\n", ncln, test_cln);
                }
            break;
        }
    }
}
